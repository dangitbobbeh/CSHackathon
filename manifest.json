{
    "manifest_version": 2,
    "name": "Cuteify",
    "version": "0.1",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ],
    "icons": {
        "16": "kitty16.png",
        "48": "kitty48.png",
        "128": "kitty.png"
    },
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "Cuteify",
        "default_icon": {
            "16": "kitty16.png",
            "48": "kitty48.png",
            "128": "kitty.png"
        }
    }
}